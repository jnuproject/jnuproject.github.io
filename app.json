{
  "expo": {
    "name": "Dream Connect",
    "slug": "dream-connect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dreamconnect",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo-round.png",
      "resizeMode": "contain",
      "backgroundColor": "#E7F3F1"
    },
    "ios": {
      "bundleIdentifier": "com.jnudream.connect",
      "buildNumber": "1",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "현재 위치를 표시하기 위해 위치 접근 권한이 필요합니다."
      },
      "config": {
        "googleMapsApiKey": "REMOVED_FOR_SECURITY"
      }
    },
    "android": {
      "package": "com.jnudream.connect",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#E7F3F1"
      },
      "config": {
        "googleMaps": {
          "apiKey": "REMOVED_FOR_SECURITY"
        }
      },
      "edgeToEdgeEnabled": true,
      "navigationBar": {
        "visible": "leanback",
        "backgroundColor": "#00000000"
      }
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-round.png",
          "resizeMode": "contain",
          "backgroundColor": "#E7F3F1"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5b0ec3e5-c30c-466b-b14d-7f661c0af6c2"
      }
    }
  }
}
